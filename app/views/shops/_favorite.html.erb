<ul id="favorite-users">
  <% favorites.each do |user| %>
    <li class="user-favorite-form">
      <%= link_to user do %>
        <div class="user-avatar">
          <%= image_tag "profile.jpg", id: "user-avatar" %>
        </div>
        <div class="user-name">
          <%= user.name %>
        </div>
      <% end %>
    </li>
  <% end %>
</ul>
